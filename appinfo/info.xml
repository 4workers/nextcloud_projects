<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>squeegee</id>
    <name>Squeegee Mod</name>
    <summary>Squeegee modification for Nextcloud</summary>
    <description><![CDATA[Later]]></description>
    <version>0.0.2</version>
    <licence>agpl</licence>
    <author mail="husband.sergey@gmail.com" homepage="https://github.com/matchish">Serhii Shliakhov</author>
    <namespace>Projects</namespace>
    <category>files</category>
    <category>organization</category>
    <bugs>https://github.com/squeegeeorg/nextcloud-squeegee</bugs>
    <dependencies>
        <nextcloud min-version="18" max-version="19"/>
    </dependencies>
    <sabre>
        <collections>
            <collection>OCA\Projects\Sabre\RootCollection</collection>
        </collections>
        <plugins>
            <plugin>OCA\Projects\Sabre\ProjectCreatePlugin</plugin>
            <plugin>OCA\Projects\Sabre\PropFindPlugin</plugin>
        </plugins>
    </sabre>
    <types>
        <dav/>
        <filesystem/>
    </types>
    <squeegee>
        <projects>
            <root>/Projects</root>
        </projects>
    </squeegee>
</info>
